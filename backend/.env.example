# Environment Variables untuk Backend (Railway)
# Copy file ini ke .env dan isi dengan nilai yang sesuai

# MongoDB Connection String
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/taskmanager?retryWrites=true&w=majority

# JWT Secret Key (gunakan string random yang panjang)
JWT_SECRET=your-super-secret-jwt-key-here-change-this

# Port (Railway akan set otomatis)
PORT=5000

# Frontend URL untuk CORS (set setelah deploy frontend)
FRONTEND_URL=https://your-frontend-app.up.railway.app
